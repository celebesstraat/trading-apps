{
  "version": 2,
  "builds": [
    {
      "src": "apps/strategywatch/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build:strategywatch"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/strategywatch/dist/$1"
    }
  ]
}